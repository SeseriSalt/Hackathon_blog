<%= render 'layouts/header' %>
<h1>作成画面</h1>
<p>ここで作る </p>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with(model: @blog, local: true) do |f| %>
      <%= f.label :content %>
      <%= f.text_field :content %>

      <%= f.submit "Create my blog", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
